  },
  "engines": {
    "node": ">= 14.17.1",
    "npm": ">= 7.13.0",
    "yarn": ">= 1.22.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run eslint:fix",
      "npm run eslint"
    ],
    "src/**/*.less": [
      "npm run stylelint:fix",
      "npm run stylelint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
