{
  "name": "react-web",
  "version": "1.0.0",
  "description": "react web",
  "keywords": [
    "React",
    "JSX",
    "Webpack",
    "React Router",
    "ESLint"
  ],
  "homepage": "https://wkylin.github.io/promotion-web",
  "main": "index.js",
  "sideEffects": true,
  "scripts": {
    "start": "cross-env BUILD_GOAL=dev NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js --stats-error-details",
    "dev:faker": "concurrently -r \"npm run start\" \"npm run faker\"",
    "build:prod": "cross-env BUILD_GOAL=prod NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details",
    "build:test": "cross-env BUILD_GOAL=test NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details",
    "analyze:build": "cross-env BUILD_GOAL=prod NODE_ENV=production USE_ANALYZE=1 webpack --config ./webpack/webpack.prod.js --stats-error-details",
    "faker": "nodemon faker/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update:yarn": "yarn upgrade-interactive --latest",
    "update:npm": "ncu -u && npm install",
    "lint:fix": "eslint --fix --cache --ext .js,jsx --format=pretty ./src && npm run lint:style",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:js": "eslint --cache --ext .js,jsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto --ignore-unknown",
    "prettier:write": "prettier --write \"src/**/*\" --ignore-unknown",
    "stylelint": "stylelint",
    "stylelint:fix": "stylelint --fix",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "serve:prod": "yarn run build:prod && serve -s dist -l 5000",
    "serve:test": "yarn run build:test && serve -s dist -l 5100",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist",
    "prepare": "husky install",
    "process": "node webpack/process",
    "dev:server": "node webpack/dev.server --stats-children",
    "pm2:server": "concurrently -r \"pm2 flush\" \"pm2 start webpack/dev.server.js --name=promotion\" \"pm2 logs --raw promotion --lines 200\""
  },
  "author": "promotion",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@koa/cors": "^3.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^2.4.2",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "dotenv-webpack": "^7.0.2",
    "es6-promise": "^4.2.8",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.7",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "isomorphic-fetch": "^3.0.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^11.0.0",
    "meow": "^8.1.2",
    "mini-css-extract-plugin": "^1.4.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "nodemon": "^2.0.12",
    "portfinder": "^1.0.28",
    "postcss": "^8.2.15",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^5.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "purgecss-webpack-plugin": "^4.0.3",
    "react-refresh": "^0.9.0",
    "react-styleguidist": "^11.1.6",
    "react-test-renderer": "^17.0.2",
    "serve": "^11.3.2",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^2.0.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.3.0",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.45.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.7.0",
    "webpack-dashboard": "^3.3.5",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@sentry/react": "^6.4.1",
    "@sentry/tracing": "^6.4.1",
    "antd": "^4.15.0",
    "constate": "^3.3.0",
    "cookies-js": "^1.2.3",
    "cross-fetch": "^3.1.4",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.21",
    "qs": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "sanitize.css": "^12.0.1",
    "trim": "^1.0.1"
  },
  "lint-staged": {
    "**/*": [
      "npm run prettier:write"
    ],
    "src/**/*.js": [
      "npm run eslint:fix",
      "npm run eslint"
    ],
    "src/**/*.less": [
      "npm run stylelint:fix",
      "npm run stylelint"
    ]
  }
}
