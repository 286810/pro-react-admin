    "portfinder": "^1.0.28",
    "postcss": "^8.2.15",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^5.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.0",
    "purgecss-webpack-plugin": "^4.0.3",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^2.7.1",
    "serve": "^12.0.0",
    "speed-measure-webpack-plugin": "^1.5.0",
