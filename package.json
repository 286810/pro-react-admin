{
  "name": "react-web",
  "version": "1.1.0",
  "description": "react web",
  "keywords": [
    "React",
    "JSX",
    "Webpack",
    "React Router",
    "ESLint"
  ],
  "homepage": "https://wkylin.github.io/promotion-web",
  "main": "index.js",
  "sideEffects": true,
  "scripts": {
    "start": "cross-env BUILD_GOAL=dev NODE_ENV=development webpack serve --config ./webpack/webpack.dev.js --stats-error-details",
    "dev:faker": "concurrently -r \"npm run start\" \"npm run faker\"",
    "build:prod": "cross-env BUILD_GOAL=prod NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details",
    "build:test": "cross-env BUILD_GOAL=test NODE_ENV=production webpack --config ./webpack/webpack.prod.js --stats-error-details",
    "analyze:build": "cross-env BUILD_GOAL=production NODE_ENV=production USE_ANALYZE=1 webpack --config ./webpack/webpack.prod.js --stats-error-details --profile --json=compilation-stats.json",
    "faker": "nodemon faker/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update:yarn": "yarn upgrade-interactive --latest",
    "update:npm": "ncu -u && npm install",
    "lint:fix": "eslint --fix --cache --ext .js,jsx --format=pretty ./src && npm run lint:style",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint:js": "eslint --cache --ext .js,jsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto --ignore-unknown",
    "prettier:check": "prettier --check \"src/**/*\" --end-of-line auto --ignore-unknown",
    "stylelint": "stylelint",
    "stylelint:fix": "stylelint --fix",
    "eslint": "eslint",
    "eslint:fix": "eslint --fix",
    "standard": "standard src/**/*",
    "standard:fix": "standard --fix src/**/*",
    "lint:json": "jsonlint --quiet src/**/*.json",
    "lint:markdown": "markdownlint --fix *.md",
    "serve:prod": "npm run build:prod && serve -s dist -l 5000",
    "serve:test": "npm run build:test && serve -s dist -l 5100",
    "http:test": "npm run build:test && npm run proxy:test",
    "proxy:test": "http-server ./dist -p 9090 --proxy https://my-json-server.typicode.com",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d dist",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "process": "node webpack/process",
    "dev:server": "node webpack/dev.server --stats-children",
    "pm2:server": "concurrently -r \"npm run faker\" \"pm2 flush\" \"pm2 start webpack/dev.server.js --name=promotion\" \"pm2 logs --raw promotion --lines 100\""
  },
  "author": "wkylin",
  "contributors": [
    {
      "name": "wkylin",
      "email": "wkylin.w@gmail.com",
      "url": "https://github.com/orgs/aresray/teams"
    }
  ],
  "bugs": {
    "url": "https://github.com/wkylin/promotion-web/issues",
    "email": "wkylin.w@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@commitlint/cli": "^12.1.4",
