    "webpack-merge": "^5.7.3",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@sentry/cli": "^1.68.0",
    "@sentry/react": "^6.11.0",
    "@sentry/tracing": "^6.11.0",
    "antd": "^4.16.9",
    "constate": "^3.3.0",
    "cookies-js": "^1.2.3",
    "cross-fetch": "^3.1.4",
    "dayjs": "^1.10.4",
    "fetch-intercept": "^2.4.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "qs": "^6.7.0",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-onclickoutside": "^6.11.2",
    "react-redux": "^7.2.4",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "sanitize.css": "^12.0.1",
    "use-http": "^1.0.21",
    "web-vitals": "^2.1.0"
  },
  "engines": {
    "node": ">= 14.17.1",
    "npm": ">= 7.13.0",
    "yarn": ">= 1.22.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run eslint:fix",
      "npm run eslint"
    ],
    "src/**/*.less": [
      "npm run stylelint:fix",
      "npm run stylelint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
